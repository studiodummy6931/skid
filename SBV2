local newWeld=function(name,p0,p1,c0,c1)
	local w=Instance.new('Weld',p0)
	w.Name=name
	w.Part0=p0
	w.Part1=p1
	w.C0=c0
	w.C1=c1
	return w
end

WeldRemote.OnServerInvoke = newWeld

