function killbot()
	ATTACK = true
	Rooted = true
	BanishGun.Parent = nil
	PistolGun.Parent = nil
	SniperGun.Parent = nil
	Katana.Parent = nil
	for i = 0, 4, 0.1 do
		Swait()
