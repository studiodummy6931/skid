		        v17649.Pitch = 4
		        v17649.Looped = false
		        local v17755 = v17649.TimePosition
		        local v17651 = v17649.TimeLength
		        if v17755 >= v17651 then
		            v17651 = v17649.TimePosition
		            v17755 = v17651 - v17649.TimeLength
		            v17649.TimePosition = v17755
		        end
		        v17649:Play()
		        local v17654 = 1
