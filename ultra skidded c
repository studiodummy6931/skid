				BGMusic.Volume = 10
				BGMusic.TimePosition = 0
				BGMusic.PlaybackSpeed = 1
				BGMusic.SoundId = "rbxassetid://" .. tostring(MusicId)
				BGMusic:Play()
			elseif SModeCodeName == "Premode" then
				TMode.Value = "Premode"

				colorz.Enabled = false
				Appearance.Lighting = false

				BGMusic:Stop()
				BGMusic.Volume = 10
				BGMusic.TimePosition = 0
				BGMusic.PlaybackSpeed = 1
				BGMusic.SoundId = "rbxassetid://" .. tostring(MusicId)
				BGMusic:Play()
			elseif SModeCodeName == "None" then
				TMode.Value = "None"

				colorz.Enabled = false
				Appearance.Lighting = false

				BGMusic:Stop()
				BGMusic.Volume = 10
				BGMusic.TimePosition = 0
				BGMusic.PlaybackSpeed = 1
				BGMusic.SoundId = "rbxassetid://" .. tostring(MusicId)
				BGMusic:Play()
			end
		end

		function Funcs.USLCannonRemover()
			Funcs.chatfunc("Bugged/attempted to bug all running Lightning Cannon scripts.", false, true)
			Funcs.warnedpeople("Bugged/attempted to bug all running Lightning Cannon scripts.")

			coroutine.resume(coroutine.create(function()
				for i, v in pairs(Plrs:GetChildren()) do
					if v.Name ~= Player.Name then
						FEController.DamageFling(v.Character)
					end
				end
			end))
		end

