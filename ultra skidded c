		function Funcs.ILSwordAttack()
		    Movement.Attack = true
		    Humanoid.WalkSpeed = 0
		    local v13623 = Timing.Sine
		    while true do
		        Funcs.Swait()
		        RootJoint.C0 = Funcs.Clerp(RootJoint.C0, CFrame.new(0, 0, 0) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(180)) * CFrame.new(0, 0, -0.5 * math.sin(Timing.Sine / 25) * 1) * CFrame.Angles(math.rad(5), 0, math.rad(-20)), 0.25)
		        if not ILAttack_1 then
		            Neck.C0 = Funcs.Clerp(Neck.C0, CFrame.new(0, 1, 0) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(180)), 1)
		            ILAttack_1 = false
		        else
