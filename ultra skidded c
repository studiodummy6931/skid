		        if 0.25 < Timing.Sine / 60 - v13623 / 60 then
		            break
		        end
		    end
		    v13684 = Timing.Sine
		    coroutine.resume(coroutine.create(function()
		        while true do
		            Funcs.Swait()
		            if 0.125 < Timing.Sine / 60 - v13684 then
