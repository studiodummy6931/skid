							RightHip.C0 = Funcs.Clerp(RightHip.C0, CFrame.new(1, -1 - BGMusic.PlaybackLoudness / 1000, -0.01) * CFrame.Angles(math.rad(0), math.rad(80), math.rad(0)) * CFrame.Angles(math.rad(-4), math.rad(0), math.rad(0)), 0.16666666666667)
							LeftHip.C0 = Funcs.Clerp(LeftHip.C0, CFrame.new(-1, -1 - BGMusic.PlaybackLoudness / 1000, -0.01) * CFrame.Angles(math.rad(0), math.rad(-80), math.rad(0)) * CFrame.Angles(math.rad(-4), math.rad(0), math.rad(0)), 0.16666666666667)
							-- v71769.C0 = Funcs.Clerp(v794.C0, CFrame.new(0.05, -1, -0.15) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(0)), 0.5)
						end
					elseif Movement.Move == true then
						ANIM = "Walk"
						if Movement.Attack == false then
							RootJoint.C0 = Funcs.Clerp(RootJoint.C0, CFrame.new(0, 0, 0) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(180)) * CFrame.new(0 + 0.25 * math.cos(Timing.Sine / 63) + 0.92 * math.sin(Timing.Sine / 95), 0, 1 + 1 * math.sin(Timing.Sine / 53)) * CFrame.Angles(math.rad(70), math.rad(0 - RootPart.RotVelocity.y), math.rad(0 - RootPart.RotVelocity.Y * 4.5 + 3 * math.cos(Timing.Sine / 47))), 0.05)
							Neck.C0 = Funcs.Clerp(Neck.C0, CFrame.new(0, 1, 0) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(180)) * CFrame.new(0, 0, 0) * CFrame.Angles(math.rad(15 - 2.5 * math.sin(Timing.Sine / 12)), math.rad(0), math.rad(0)), 0.05)
							RightShoulder.C0 = Funcs.Clerp(RightShoulder.C0, CFrame.new(1.5, 0.5, 0) * CFrame.Angles(math.rad(180 + BGMusic.PlaybackLoudness / 7.5), math.rad(0), math.rad(10)) * CFrame.new(-0.5, 0, 0) * CFrame.Angles(math.rad(0), math.rad(90), math.rad(0)), 0.16666666666667)
							LeftShoulder.C0 = Funcs.Clerp(LeftShoulder.C0, CFrame.new(-1.4, 0.5, 0) * CFrame.Angles(math.rad(180 + BGMusic.PlaybackLoudness / 7.5), math.rad(0), math.rad(-10)) * CFrame.new(0.5, 0, 0) * CFrame.Angles(math.rad(0), math.rad(-90), math.rad(0)), 0.16666666666667)
							RightHip.C0 = Funcs.Clerp(RightHip.C0, CFrame.new(1, -0.5 - 0.02 * math.sin(Timing.Sine / 12), -0.5) * CFrame.Angles(math.rad(-10 - 2.5 * math.sin(Timing.Sine / 21)), math.rad(90), math.rad(0)) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.05)
							LeftHip.C0 = Funcs.Clerp(LeftHip.C0, CFrame.new(-1, -1 - 0.02 * math.sin(Timing.Sine / 12), -0.01) * CFrame.Angles(math.rad(-20 - 2.5 * math.sin(Timing.Sine / 51)), math.rad(-90), math.rad(0)) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 0.05)
							-- v71769.C0 = Funcs.Clerp(v794.C0, CFrame.new(0.05, -1, -0.15) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(0)), 0.5)
						end
					end
				elseif TMode.Value == "Premode" then
					Movement.Attack = false
					Appearance.Gun.Transparency = 1
					RightHip.C0 = Funcs.Clerp(RightHip.C0, CF(1, 0.35 - 0.05 * math.cos(Timing.Sine / 32), -0.7) * ANGLES(math.rad(-20), math.rad(75), math.rad(0)) * ANGLES(math.rad(-3), math.rad(0 - 1 * math.cos(Timing.Sine / 56)), math.rad(25 - 2 * math.cos(Timing.Sine / 32))), 0.1)
					LeftHip.C0 = Funcs.Clerp(LeftHip.C0, CF(-1, -1.125 - 0.05 * math.cos(Timing.Sine / 32), -0.6) * ANGLES(math.rad(-105), math.rad(-75), math.rad(0)) * ANGLES(math.rad(-3), math.rad(0 - 1 * math.cos(Timing.Sine / 36)), math.rad(-25 + 2 * math.cos(Timing.Sine / 32))), 0.1)
					RootJoint.C0 = Funcs.Clerp(RootJoint.C0, CFrame.new(0, 0, 0) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(180)) * CF(0, 0, -1.3 + 0.05 * math.cos(Timing.Sine / 32)) * ANGLES(math.rad(7.5 - 2 * math.cos(Timing.Sine / 32)), math.rad(0), math.rad(0)), 0.1)
					Neck.C0 = Funcs.Clerp(Neck.C0, CFrame.new(0, 1, 0) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(180)) * ANGLES(math.rad(25 - 2 * math.cos(Timing.Sine / 37)), math.rad(0 + 1 * math.cos(Timing.Sine / 58)), math.rad(0 + 2 * math.cos(Timing.Sine / 53))), 0.1)
					RightShoulder.C0 = Funcs.Clerp(RightShoulder.C0, CF(1.35, 0.5 + 0.025 * math.cos(Timing.Sine / 45), 0) * ANGLES(math.rad(165 + 5 * math.cos(Timing.Sine / 74)), math.rad(1 - 3 * math.cos(Timing.Sine / 53)), math.rad(-15 + 3 * math.cos(Timing.Sine / 45))), 0.1)
					LeftShoulder.C0 = Funcs.Clerp(LeftShoulder.C0, CF(-1.35, 0.5 + 0.025 * math.cos(Timing.Sine / 45), 0) * ANGLES(math.rad(165 - 3 * math.cos(Timing.Sine / 73)), math.rad(2 - 1 * math.cos(Timing.Sine / 55)), math.rad(15 - 3 * math.cos(Timing.Sine / 45))), 0.1)
				end
			end
		end)

		pcall(function()
			Movement.Attack = true
			BGMusic:Stop()
			Funcs.Warning()

			Funcs.Fly()
			Funcs.FixCamera(CameraRootPart, 0.5, 400)
			CharClones.CharacterCloneLoading:Destroy()

			Funcs.spawnwave(Torso.Position)
			Funcs.CreateSound(157878578, Character, 10, 1, false)
			Funcs.SwitchModeEffect()
			Funcs.attackone()
			_G.SCRIPT_LOADED = true
			Movement.Attack = false
		end)

		spawn(function()
			do
				BGMusic:Stop()
				
				warn("-------------------------------------------------------------------------------------------------------------------------")
				print("\n\n\n                                           _______  _______             _______  _______ \n                                 |\\     /|(  ____ \\(  ____ \\  |\\     /|(  ____ \\(  ____ \\\n                                 | )   ( || (    \\/| (    \\/  ( \\   / )| (    \\/| (    \\/\n                                 | (___) || (_____ | |         \\ (_) / | (_____ | (__    \n                                 |  ___  |(_____  )| |          ) _ (  (_____  )|  __)   \n                                 | (   ) |      ) || |         / ( ) \\       ) || (      \n                                 | )   ( |/\\____) || (____/\\  ( /   \\ )/\\____) || (____/\\\n                                 |/     \\|\\_______)(_______/  |/     \\|\\_______)(_______/\n                                                        \n\n\n                                \194\169 2020 - 2021 TigerX. Feel free to leak and edit my scripts.\n\n                                                   Made/Edited in Japan.\n \n                                                                                                          \n")
				warn("-------------------------------------------------------------------------------------------------------------------------")
				warn("Hyperskidded Cannon XSE-FE (Filtering Enabled) has been started")
				warn("(HSC XSE-FE) Current version: " .. SCRIPTVERSION .. "")
				warn("(HSC XSE-FE) Current user: " .. game.Players.LocalPlayer.Name .. "")
				warn("-------------------------------------------------------------------------------------------------")
				warn("-------------------------------------------------------------------------------------------------------------------------")
				
				if Options.ChatEnabled then
					Funcs.Serv("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Hyperskidded Cannon XSE-FE (Filtering Enabled) " .. SCRIPTVERSION .. " by Breakfast/ItzNotShadow01. Original(HSC XSE) by SUPER_TIGERPRO/super_tiger123456 loaded.", "All")
				end

				FEController.Teleport(ReanimCharacter.HumanoidRootPart, ORIGIN_POS * CFrame.new(0, 150, 0))
				Funcs.IntroX()
				
