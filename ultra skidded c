		function Funcs.Warning()
			coroutine.resume(coroutine.create(function()
				CloneCharacterGUI.Enabled = false

				local IntroWarning = Instance.new("ScreenGui")
				IntroWarning.Parent = game.Players.LocalPlayer.PlayerGui
				IntroWarning.Name = tostring(math.random())

				local IntroWarningFilter = Instance.new("Frame")
				IntroWarningFilter.Parent = IntroWarning
				IntroWarningFilter.Name = "Filter"
				IntroWarningFilter.Position = UDim2.new(0, 0, -0.1, 0)
				IntroWarningFilter.Size = UDim2.new(1, 0, 1.2, 0)
				IntroWarningFilter.BorderSizePixel = 0
				IntroWarningFilter.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				IntroWarningFilter.BorderColor3 = Color3.fromRGB(0, 0, 0)
				IntroWarningFilter.BackgroundTransparency = 0

				local IntroWarningBackground = Instance.new("Frame")
				IntroWarningBackground.Parent = IntroWarning
				IntroWarningBackground.Name = "Background"
				IntroWarningBackground.Position = UDim2.new(0, 0, -0.1, 0)
				IntroWarningBackground.Size = UDim2.new(1, 0, 1.2, 0)
				IntroWarningBackground.BorderSizePixel = 0
				IntroWarningBackground.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				IntroWarningBackground.BorderColor3 = Color3.fromRGB(0, 0, 0)

				local IntroWarningContent = Instance.new("Frame")
				IntroWarningContent.Parent = IntroWarning
				IntroWarningContent.Name = "Content"
				IntroWarningContent.Size = UDim2.new(1, 0, 1, 0)
				IntroWarningContent.BorderSizePixel = 0
				IntroWarningContent.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				IntroWarningContent.BackgroundTransparency = 1
				IntroWarningContent.BorderColor3 = Color3.fromRGB(0, 0, 0)

				local IntroWarningText1 = Instance.new("TextButton")
				IntroWarningText1.Parent = IntroWarningContent
				IntroWarningText1.Name = "TextButton1"
				IntroWarningText1.Position = UDim2.new(0.5, 0, 0.345, 0)
				IntroWarningText1.Font = "ArialBold"
				IntroWarningText1.Text = "[WARNING]"
				IntroWarningText1.TextSize = 38
				IntroWarningText1.TextColor3 = Color3.fromRGB(255, 0, 0)
				IntroWarningText1.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
				IntroWarningText1.TextStrokeTransparency = 0.3

				local IntroWarningText2 = Instance.new("TextButton")
				IntroWarningText2.Parent = IntroWarningText1
				IntroWarningText2.Name = "TextButton2"
				IntroWarningText2.Position = UDim2.new(0, 0, 0, 190)
				IntroWarningText2.Font = "ArialBold"
				IntroWarningText2.Text = "There is no guarantee that this script is still overpowered due to it's age"
				IntroWarningText2.TextSize = 38
				IntroWarningText2.TextColor3 = Color3.fromRGB(255, 255, 255)
				IntroWarningText2.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
				IntroWarningText2.TextStrokeTransparency = 0.3

				Config = {
				    Fade_Time = {Value = 0.8}, 
				    Display_Time = {Value = 0.8}, 
				    Padding_Time = {Value = 0.5}
				}

				local function FilterFade(p1, p2, p3)
				    for i = p1, p2, 0.03 / p3 do
				        IntroWarningFilter.BackgroundTransparency = i
				        IntroWarningText1.TextTransparency = i
				        IntroWarningText1.TextStrokeTransparency = i
				        IntroWarningText2.TextTransparency = i
				        IntroWarningText2.TextStrokeTransparency = i
				        wait()
				    end
				    IntroWarningFilter.BackgroundTransparency = p2
				end

				IntroWarningFilter.Visible = true

				for i, v in pairs(IntroWarningContent:GetChildren()) do
				    v.Visible = false
				end

				wait(Config.Padding_Time.Value)

				for i, v in pairs(IntroWarningContent:GetChildren()) do
				    v.Visible = true
				    FilterFade(1, 0, -Config.Fade_Time.Value)
				    wait(Config.Display_Time.Value)
				    FilterFade(0, 1, Config.Fade_Time.Value)
				    v.Visible = false
				    wait(Config.Display_Time.Value)
				end

				wait(Config.Padding_Time.Value)
				IntroWarningBackground.Visible = false
				FilterFade(0, 1, Config.Fade_Time.Value)
				IntroWarning:Remove()
			end))
		end

		function Funcs.IntroX()
			Movement.Attack = true
			Premode = true
			Humanoid.WalkSpeed = 0
			CloneCharacterGUI.Enabled = false
			DaGui.Enabled = false
			WEAPONGUI.Enabled = false
			wait(3) -- 4
			Funcs.SwitchMode("夜に駆ける", 7092435374) -- 679355361
			CloneCharacterGUI.Enabled = true
			repeat Funcs.Swait() until BGMusic.TimePosition >= 54
			for i=0, 0.1 / 3, 0.002 / Animation_Speed do
				Funcs.Swait()
				local Z = math.random(0,255)
				Funcs.WAEFFX({Time = 15, EffectType = "Block", Size = VT(0,0,0), Size2 = VT(0.2,20,0.2), Transparency = 0, Transparency2 = 1, CFrame = Torso.CFrame*CF(0,0,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))), MoveToPos = nil, RotationX = math.random(-20,20), RotationY = math.random(-20,20), RotationZ = math.random(-20,20), Material = "Neon", Color = Color3.fromRGB(Z,Z,Z), SoundID = nil, SoundPitch = nil, SoundVolume = nil})			
