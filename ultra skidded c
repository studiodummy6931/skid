		function Funcs.Blood()
			Movement.Attack = true
			Humanoid.WalkSpeed = 0
			for i=0, 0.1 / 3, 0.002 / Animation_Speed do
				Funcs.Swait()
				local Z = math.random(0,255)
				Funcs.WAEFFX({Time = 15, EffectType = "Block", Size = VT(0,0,0), Size2 = VT(0.2,20,0.2), Transparency = 0, Transparency2 = 1, CFrame = Torso.CFrame*CF(0,0,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))), MoveToPos = nil, RotationX = math.random(-20,20), RotationY = math.random(-20,20), RotationZ = math.random(-20,20), Material = "Neon", Color = Color3.fromRGB(Z,Z,Z), SoundID = nil, SoundPitch = nil, SoundVolume = nil})			
				RootJoint.C0 = Funcs.Clerp(RootJoint.C0,Welds.Defaults.RootJoint.C0 * CF(0 , -1.7 , -1.3) * ANGLES(RAD(20), RAD(0), RAD(0)), 0.35 / Animation_Speed)
				Neck.C0 = Funcs.Clerp(Neck.C0, Welds.Defaults.Neck.C0 * CF(0, 0, 0) * ANGLES(RAD(30+MATHR(-15,15)), RAD(0+MATHR(-15,15)), RAD(0+MATHR(-15,15))), 0.35 / Animation_Speed)
				RightShoulder.C0 = Funcs.Clerp(RightShoulder.C0,CFrame.new(1.5,0.5,0) * CFrame.Angles(math.rad(135 + 8.5 * math.cos(Timing.Sine/50)),math.rad(0),math.rad(25)) * Welds.Defaults.RightShoulder.C0,0.7/3)
				LeftShoulder.C0 = Funcs.Clerp(LeftShoulder.C0, CF(-0.5+math.random(-15,15)/100, 0.5+math.random(-15,15)/100, -0.5+math.random(-15,15)/100) * ANGLES(RAD(140), RAD(0), RAD(20)) * Welds.Defaults.LeftShoulder.C0, 0.35 / Animation_Speed)
				RightHip.C0 = Funcs.Clerp(RightHip.C0, CF(0.99, -1, -0.7) * ANGLES(RAD(-90+20), RAD(0), RAD(0)) * ANGLES(RAD(0), RAD(90), RAD(0)), 0.35 / Animation_Speed)
				LeftHip.C0 = Funcs.Clerp(LeftHip.C0, CF(-0.99, -1 , -0.7) * ANGLES(RAD(-90+20), RAD(0), RAD(0)) * ANGLES(RAD(0), RAD(-90), RAD(0)), 0.35 / Animation_Speed)
			end
			Funcs.WAEFFX({Time = 35, EffectType = "Block", Size = VT(0,0,0), Size2 = VT(65,65,65), Transparency = 0, Transparency2 = 1, CFrame = Torso.CFrame*CF(0,0,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))), MoveToPos = nil, RotationX = math.random(-360,360), RotationY = math.random(-360,360), RotationZ = math.random(-360,360), Material = "Neon", Color = Color3.fromRGB(255,255,255), SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			Funcs.WAEFFX({Time = 35, EffectType = "Block", Size = VT(0,0,0), Size2 = VT(65,65,65), Transparency = 0, Transparency2 = 1, CFrame = Torso.CFrame*CF(0,0,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))), MoveToPos = nil, RotationX = math.random(-360,360), RotationY = math.random(-360,360), RotationZ = math.random(-360,360), Material = "Neon", Color = Color3.fromRGB(0,0,0), SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			Funcs.WAEFFX({Time = 35, EffectType = "Block", Size = VT(0,0,0), Size2 = VT(3,205,3), Transparency = 0, Transparency2 = 1, CFrame = Torso.CFrame*CF(0,0,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))), MoveToPos = nil, RotationX = math.random(-20,20), RotationY = math.random(-20,20), RotationZ = math.random(-20,20), Material = "Neon", Color = Color3.fromRGB(255,255,255), SoundID = nil, SoundPitch = nil, SoundVolume = nil})	
