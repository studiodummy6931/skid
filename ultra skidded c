		function Funcs.roar()
		    coroutine.resume(coroutine.create(function()
		        Movement.Attack = true
		        v128 = false
		        Humanoid.WalkSpeed = 0
		        local v3441 = Funcs.CreateSound(6070263388, Character, 10, 1, false)
		        Funcs.warnedpeople("DIE Y'ALL.", true)
		        v3441.EmitterSize = 10000
		        coroutine.resume(coroutine.create(function()
		            Funcs.Swait(60)
		            Funcs.MDR(Head.Position, 10000)
		            -- Funcs.MDR(v727.Position, 10000)
		        end))
		        while true do
		            local v3467 = Vector3.new(math.random(-10, 10) / 50, math.random(-10, 10) / 50, math.random(-10, 10) / 50)
		            Funcs.Serv("Workspace").CurrentCamera.Focus = Funcs.Serv("Workspace").CurrentCamera.Focus + v3467
		            Funcs.Serv("Workspace").CurrentCamera.CFrame = Funcs.Serv("Workspace").CurrentCamera.CFrame + v3467
		            RootJoint.C0 = Funcs.Clerp(RootJoint.C0, CFrame.new(0, 0, 0) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(180)) * CFrame.new(0, 0, 3 - 0.5 * math.sin(Timing.Sine / 50)) * CFrame.Angles(math.rad(-45), math.rad(0), math.rad(0)), 1)
		            Neck.C0 = Funcs.Clerp(Neck.C0, CFrame.new(0, 1, 0) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(180)) * CFrame.new(0, 0, 0) * CFrame.Angles(math.rad(-45 + math.random(-5, 5)), math.rad(math.random(-5, 5)), math.rad(math.random(-5, 5))), 1)
		            RightShoulder.C0 = Funcs.Clerp(RightShoulder.C0, CFrame.new(1.5, 0.5, 0) * CFrame.Angles(math.rad(-45), math.rad(0), math.rad(0)) * CFrame.new(-0.5, 0, 0) * CFrame.Angles(math.rad(0), math.rad(90), math.rad(0)), 1)
		            LeftShoulder.C0 = Funcs.Clerp(LeftShoulder.C0, CFrame.new(-1.5, 0.5, 0) * CFrame.Angles(math.rad(-45), math.rad(0), math.rad(0)) * CFrame.new(0.5, 0, 0) * CFrame.Angles(math.rad(0), math.rad(-90), math.rad(0)), 1)
		            RightHip.C0 = Funcs.Clerp(RightHip.C0, CFrame.new(1, -1, -0.01) * CFrame.Angles(math.rad(-45), math.rad(80), math.rad(0)), 1)
		            LeftHip.C0 = Funcs.Clerp(LeftHip.C0, CFrame.new(-1, -1, -0.01) * CFrame.Angles(math.rad(-35), math.rad(-80), math.rad(0)), 1)
		            -- v794.C0 = Funcs.Clerp(v794.C0, CFrame.new(0.05, -1, -0.15) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(0)), 1)
		            Funcs.Serv("RunService").RenderStepped:Wait()
		            if 5.5 < v3441.TimePosition then
		                break
		            end
		            if not v3441 then
		                break
		            end
		        end
		        v3610 = game
		        if not v3441:IsDescendantOf(v3610:GetService("Workspace")) then
		            v3441:Destroy()
		        end


		        Humanoid.WalkSpeed = 50
		        Movement.Attack = false
		    end))
		end

