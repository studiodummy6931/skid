		function Kill(dude)
			coroutine.resume(coroutine.create(function()
				if dude and dude.Name ~= Character.Name and dude.Name ~= Player.Name then
					local h = dude:FindFirstChildOfClass("Humanoid")
					local t = dude:FindFirstChild("Torso") or dude:FindFirstChild("UpperTorso") or dude:FindFirstChild("HumanoidRootPart")
					local deathp = Instance.new("Part",Effects) deathp.Anchored = true deathp.Size = Vector3.new() deathp.Transparency = 1 deathp.CanCollide = false deathp.CFrame = t.CFrame
					coroutine.wrap(function()
						deathp:Destroy()
					end)
					if h then
						if dude then
							Funcs.CreateSound(206082273, deathp, 5, .75)
							for i = 0, math.random(3,7) do
								Funcs.WAEFF({Time = math.random(26.25,51.25), EffectType = "Sphere", Size = Vector3.new(2.5,2.5,2.5), Size2 = Vector3.new(1.25,20,1.25), Transparency = 0, Transparency2 = 1, CFrame = deathp.CFrame*CFrame.Angles(math.rad(math.random(0,360)),math.rad(math.random(0,360)),math.rad(math.random(0,360))), MoveToPos = nil, RotationX = nil, RotationY = nil, RotationZ = nil, Material = "Neon", Color = Color3.new(1,0,0), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 35})
							end
							Funcs.WAEFF({Time = math.random(18,20.5), EffectType = "Sphere", Size = Vector3.new(2.5,2.5,2.5), Size2 = Vector3.new(10,10,10), Transparency = 0.6, Transparency2 = 1, CFrame = deathp.CFrame, MoveToPos = nil, RotationX = nil, RotationY = nil, RotationZ = nil, Material = "Neon", Color = Color3.new(1,0,0), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 35})
							for i = 0, math.random(5,9) do
								Funcs.WAEFF({Time = math.random(9,10.25), EffectType = "Sphere", Size = Vector3.new(4.5,4.5,4.5), Size2 = Vector3.new(2,2,2), Transparency = 0, Transparency2 = 1, CFrame = deathp.CFrame, MoveToPos = deathp.CFrame*CFrame.new(math.random(-95,95),math.random(-95,95),math.random(-95,95)).p, RotationX = nil, RotationY = nil, RotationZ = nil, Material = "Neon", Color = Color3.new(1,0,0), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 45, SizeBoomerang = 40})
							end
						end
					end
				end
				FEController.DamageFling(dude)
			end))
		end

		function ApplyAoE(POSITION,RANGE)
			coroutine.resume(coroutine.create(function()
				for index, CHILD in pairs(workspace:GetDescendants()) do
					if CHILD.ClassName == "Model" and CHILD ~= Character then
						local HUM = CHILD:FindFirstChildOfClass("Humanoid")
						if HUM then
							local TORSO = CHILD:FindFirstChild("Torso") or CHILD:FindFirstChild("UpperTorso")
							if TORSO then
								if (TORSO.Position - POSITION).Magnitude <= RANGE then
									Kill(CHILD)
								end
							end
						end
					end
				end
			end))
		end

		function Funcs.MDR(p289, p290)
			pcall(function()
			    for v7859, v7860 in pairs(Plrs:GetChildren()) do
			    	v7860 = v7860.Character.Humanoid

			        local v7863 = v7860:IsDescendantOf(Character)
			        local v7863 = v7860:IsDescendantOf(Character)

			        if v7860:IsDescendantOf(Player.Character) or v7860:IsDescendantOf(Character) or not v7860:IsA("Humanoid") or not v7860.Parent:FindFirstChild("HumanoidRootPart") or (not v7860.Parent:FindFirstChild("Torso") and not v7860.Parent:FindFirstChild("UpperTorso")) or (v7860.Parent.HumanoidRootPart.Position - p289).Magnitude > p290 then
			        	continue
			        end

			        local OriginArchivable = v7860.Parent.Archivable
			        local OriginChar = v7860.Parent

			        v7860.Parent.Archivable = true

			        v7860 = v7860.Parent:Clone()
			        v7860.Parent = workspace
			        v7860.HumanoidRootPart.CFrame = OriginChar.HumanoidRootPart.CFrame

			        OriginChar.Archivable = OriginArchivable

			        v7865 = v7860.HumanoidRootPart
			        v7865 = (v7860.HumanoidRootPart.Position - p289).Magnitude
			        local v7969 = p290 + v7860.HumanoidRootPart.Size.Magnitude
			        v7865 = Instance.new
			        local v7871 = v7865("Part")
			        v7871.Anchored = true
			        v7871.Transparency = 1
			        v7871.CanCollide = false
			        v7871.CFrame = v7860.HumanoidRootPart.CFrame
			        Funcs.CreateSound(206082273, Character, 5, 0.75).PlayOnRemove = true
			        local v7875 = 1
			        local v7980 = 10
			        local v7876 = 1
			        for v7875 = 1, 10, 1 do
			            local v7879 = Instance.new("Part")
			            v7879.Name = Funcs.randomstring()
			            v7879.Anchored = true
			            v7879.Position = v7871.Position
			            v7879.Shape = "Ball"
			            v7879.Color = Color3.new(0, 0, 0)
			            v7879.Material = "Neon"
			            v7879.CastShadow = false
			            v7879.Size = Vector3.new(v7875 * 2.5, v7875 * 2.5, v7875 * 2.5)
			            v7879.CanCollide = false
			            v7879.Transparency = 0.75
			            v7879.Parent = Funcs.Serv("Workspace")
			            local v7891 = 1.5
			            local v7892 = v7875 / 10
			            Funcs.Serv("Debris"):AddItem(v7879, v7891 - v7892)
			            for v7894 = 1, 3, 1 do
			                v7891 = PlayerGui["0.57193589159619"]["0.7590469604785"].ierofjqwoiuorewqjofjweor

			                local v7899 = v7891.ertfwetfretfretewtwtewrtret:Clone()

			                v7899.Color = BrickColor.new("Black").Color
			                v7899.Trail.Color = ColorSequence.new(BrickColor.new("Black").Color, Color3.new(1, 1, 1))
			                v7899.attachment.ParticleEmitter.Color = ColorSequence.new(BrickColor.new("Black").Color, Color3.new(1, 1, 1))
			                v7899.CFrame = v7879.CFrame * CFrame.Angles(math.rad(math.random(0, 360)), math.rad(math.random(0, 360)), math.rad(math.random(0, 360))) * CFrame.new(0, 1, 0)
			                v7899.Parent = WorkspaceEffects
			                v7899.Velocity = CFrame.new(v7899.Position, v7879.Position).LookVector * 250

			                Funcs.Serv("Debris"):AddItem(v7899, 1.33)
			            end
			        end
			        coroutine.resume(coroutine.create(function()
			        	FEController.DamageFling(OriginChar)
			        end))
			        v7871.Parent = Funcs.Serv("Workspace")
			        pcall(function()
			            v7871:Destroy()
			        end)
			        local v7941 = v7860:IsDescendantOf(game)
			        v7941 = v7860.Parent
			        v7879 = "Workspace"
			        local v7943 = game:GetService(v7879)
			        v7941 = v7860.Parent
			        v7895 = "Workspace"
			        local v7945 = game:GetService(v7895)
			        v7943 = v7945.Terrain
			        if v7941 ~= v7943 then
			            v7941 = v7860.HumanoidRootPart
			            v7941:Destroy()
			            local v7946 = v7860
			            local ClonedChar = v7946
			            while true do
			                v7946 = v7946.Parent
			                v7943 = v7946.Parent
			                v7945 = game
			                local v7948 = v7945:GetService("Workspace")
			                if v7943 ~= v7948 then
			                    break
			                end
			                v7943 = v7946.Parent
			                v7896 = "Workspace"
			                v7948 = game:GetService(v7896).Terrain
			                if v7943 ~= v7948 then
			                    break
			                end
			            end
			            coroutine.resume(coroutine.create(function()
			                local v7951 = 0
			                while true do
			                    v7951 = v7951 + Funcs.Serv("RunService").RenderStepped:Wait()
			                    if 3 < v7951 then
			                        break
			                    end
			                end
			                v7956 = RayCast_1
			                -- v7956:FireServer(v7946.Name)
			            end))
			            ClonedChar:Destroy()
			        else
			            local v7962 = v7860:IsDescendantOf(game)
			            if not v7962 then
			                v7962 = v7860.HumanoidRootPart
			                v7962:Destroy()
			                v7860:Destroy()
			            end
			        end
			    end
			end)
		end

		function Funcs.MDRW(p289, p290)
			pcall(function()
			    for v7859, v7860 in pairs(Funcs.Serv("Workspace"):GetDescendants()) do
			        local v7863 = v7860:IsDescendantOf(Character)
			        local v7863 = v7860:IsDescendantOf(Character)

			        if v7860:IsDescendantOf(Player.Character) or v7860:IsDescendantOf(Character) or not v7860:IsA("Humanoid") or not v7860.Parent:FindFirstChild("HumanoidRootPart") or (not v7860.Parent:FindFirstChild("Torso") and not v7860.Parent:FindFirstChild("UpperTorso")) or (v7860.Parent.HumanoidRootPart.Position - p289).Magnitude > p290 then
			        	continue
			        end

			        local OriginArchivable = v7860.Parent.Archivable
			        local OriginChar = v7860.Parent

			        v7860.Parent.Archivable = true

			        v7860 = v7860.Parent:Clone()
			        v7860.Parent = workspace
			        v7860.HumanoidRootPart.CFrame = OriginChar.HumanoidRootPart.CFrame

			        OriginChar.Archivable = OriginArchivable

			        v7865 = v7860.HumanoidRootPart
			        v7865 = (v7860.HumanoidRootPart.Position - p289).Magnitude
			        local v7969 = p290 + v7860.HumanoidRootPart.Size.Magnitude
			        v7865 = Instance.new
			        local v7871 = v7865("Part")
			        v7871.Anchored = true
			        v7871.Transparency = 1
			        v7871.CanCollide = false
