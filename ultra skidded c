			v7658[18] = "X"
			v7658[19] = "C"
			v7658[20] = "V"
			v7658[21] = "B"
			v7658[22] = "N"
			v7658[23] = "M"
			local v7700 = CreateLabel(CreateFrame(WEAPONGUI, 1, 2, UDim2.new(0.8, 0, 0.9, 0), UDim2.new(0.26, 0, 0.07, 0), Color3.new(0, 0, 0), Color3.new(0, 0, 0), "Skill Frame"), "[XSE-FE]", BrickColor.new("New Yeller").Color, 8.5, Enum.Font.Antique, 0, 2, 0.5, "Skill text")

			for v7701 = 1, #v7658, 1 do
				v7668 = 0
				v7670 = 0.9
				v7672 = 0.04
				v7671 = v7672 * v7701
				v7669 = v7670 - v7671
				v7670 = 0
				v7671 = 0
				v7672 = 0
				local v7737 = CreateLabel(CreateFrame(WEAPONGUI, 1, 2, UDim2.new(0.8, v7668, v7669, v7670), UDim2.new(0.26, 0, 0.07, v7671), Color3.new(0, 0, 0), Color3.new(0, 0, v7672), "Skill Frame"), "[" .. v7658[v7701] .. "]", BrickColor.new("New Yeller").Color, 7.5, Enum.Font.Antique, 0, 2, 0.5, "Skill text")
			end

			FixEngineButton.MouseButton1Down:Connect(function()
				FEController.FixCharacter()
			end)

			GoodbyeButton.MouseButton1Down:Connect(function()
				Player:kick("")
			end)

			coroutine.resume(coroutine.create(function()
				local v7248 = Instance.new("LocalScript", FPSMeter)

				Funcs.Serv("RunService").Heartbeat:Connect(function(fps) 
					if v7248.Parent then
						v7248.Parent.Text = "\226\148\131" .. math.floor(1 / fps) .. "\226\148\131"
					end
				end)
			end))
		end

		Player.Chatted:Connect(function(Message)
			Funcs.chatfunc(Message, false, true)
			
			if Options.ChatEnabled then
				Funcs.Serv("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("[" .. NameMode.Text .. "]", "All")
			end
		end)

		
		Plrs.PlayerChatted:Connect(function(chatType, player, message, targetPlayer)
			if Options.ChatEventFling then
			    if player.Name ~= Player.Name and not Player:IsFriendsWith(player.UserId) then
			        FEController.DamageFling(player.Character)
			        
			        coroutine.resume(coroutine.create(function()
			            if not chatDelay and Options.ChatEnabled then
			                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w " .. player.Name .." " .. ChatFlingMessages[math.random(1, #ChatFlingMessages)] .. " - Hyperskidded Cannon XSE", "All")
			                
			                chatDelay = true
			                wait(3)
			                chatDelay = false
			            end
			        end))
			    end
			end
		end)

