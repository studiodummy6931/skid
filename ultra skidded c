		function Funcs.CheckArchivable(p28)
		    if p28.Archivable == false then
		        p28.Archivable = true
		    end

		    p28:GetPropertyChangedSignal("Archivable"):Connect(function()
		        if p28.Archivable == false then
		            p28.Archivable = true
		        end
		    end)
		end
		function Funcs.RandStX()
			local length = MATHR(10,20)
			local array = {}
			for i = 1,length do
				array[i] = string.char(MATHR(14,126))
			end
			return table.concat(array)
		end
		function Funcs.RandStXR(length)
			local array = {}
			for i = 1,length do
				array[i] = string.char(MATHR(14,255))
			end
			return table.concat(array)
		end
		function Funcs.RandStXGR(length,randmin,randmax)
			local array = {}
			for i = 1,length do
				array[i] = string.char(MATHR(randmin,randmax))
			end
			return table.concat(array)
		end
		function Funcs.randomstring()
			local v149 = {}
			local v150 = 1
			local v151 = math.random(10, 20)
			local v152 = 1
			for v150 = v150, v151, v152 do
				v149[v150] = string.char(math.random(14, 126))
			end
			v151 = table.concat
			v152 = v149
			return v151(v152)
		end
		function Funcs.otherrandomstring(p5)
			local v168 = {}
			local v169 = 1
			local v170 = p5
			local v171 = 1
			for v169 = v169, v170, v171 do
				v168[v169] = string.char(MATHR(14, 255))
			end
			v170 = table.concat
			v171 = v168
			return v170(v171)
		end

