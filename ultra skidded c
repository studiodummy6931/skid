		function Funcs.SwitchModeEffectHSC()
		    Movement.Attack = true
		    Funcs.CreateSoundPp(363808674, Character, 9000000000, 1, false)
		    for v19204 = 0, 0.1, 0.05 do
		        Funcs.Swait()
		        RootJoint.C0 = Funcs.Clerp(RootJoint.C0, Welds.Defaults.RootJoint.C0 * CF(0, 0, 0 + 0.35 * COS(Timing.Sine / 2)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / 3)
		        Neck.C0 = Funcs.Clerp(Neck.C0, Welds.Defaults.Neck.C0 * CF(0, 0, 0) * ANGLES(RAD(5), RAD(25), RAD(0)), 1 / 3)
		        RightShoulder.C0 = Funcs.Clerp(RightShoulder.C0, CF(1.25, 0.5, -0.5) * ANGLES(RAD(100), RAD(0), RAD(-50)) * Welds.Defaults.RightShoulder.C0, 1 / 3)
		        LeftShoulder.C0 = Funcs.Clerp(LeftShoulder.C0, CF(-1.25, 0.35, -0.35) * ANGLES(RAD(70), RAD(0), RAD(60)) * Welds.Defaults.LeftShoulder.C0, 1 / 3)
		        RightHip.C0 = Funcs.Clerp(RightHip.C0, CF(1, -1 - 0.35 * COS(Timing.Sine / 2), -0.01) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / 3)
		        LeftHip.C0 = Funcs.Clerp(LeftHip.C0, CF(-1, -1 - 0.35 * COS(Timing.Sine / 2), -0.01) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / 3)
		    end
		    Funcs.Swait(3) -- 6
		    Funcs.WAEFFX({Time = 60, EffectType = "Sphere", Size = VT(0.5, 0.55, 0.5), Size2 = VT(0.5, 4, 0.5), Transparency = 0, Transparency2 = 1, CFrame = Head.CFrame * CF(0.4, 0.2, -0.8), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1, 0, 0), SoundID = 743521450, SoundPitch = 1.15, SoundVolume = 6})    
		    Funcs.WAEFFX({Time = 60, EffectType = "Sphere", Size = VT(0.55, 0.5, 0.5), Size2 = VT(4, 0.5, 0.5), Transparency = 0, Transparency2 = 1, CFrame = Head.CFrame * CF(0.4, 0.2, -0.8), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1, 0, 0), SoundID = 743521450, SoundPitch = 0.95, SoundVolume = 6})
		    Funcs.CreateSoundPp(363808674, Character, 9000000000, 1, false)
		    for v19380 = 0, 0.1, 0.05 do
		        Funcs.Swait()
		        RootJoint.C0 = Funcs.Clerp(RootJoint.C0, Welds.Defaults.RootJoint.C0 * CF(0, 0, 0 + 0.35 * COS(Timing.Sine / 2)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / 3)
		        Neck.C0 = Funcs.Clerp(Neck.C0, Welds.Defaults.Neck.C0 * CF(0, 0, 0) * ANGLES(RAD(5), RAD(-25), RAD(0)), 1 / 3)
		        RightShoulder.C0 = Funcs.Clerp(RightShoulder.C0, CF(1.25, 0.5, -0.5) * ANGLES(RAD(100), RAD(0), RAD(-90)) * Welds.Defaults.RightShoulder.C0, 1 / 3)
		        LeftShoulder.C0 = Funcs.Clerp(LeftShoulder.C0, CF(-1.25, 0.35, -0.35) * ANGLES(RAD(70), RAD(0), RAD(90)) * Welds.Defaults.LeftShoulder.C0, 1 / 3)
		        RightHip.C0 = Funcs.Clerp(RightHip.C0, CF(1, -1 - 0.35 * COS(Timing.Sine / 2), -0.01) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / 3)
		        LeftHip.C0 = Funcs.Clerp(LeftHip.C0, CF(-1, -1 - 0.35 * COS(Timing.Sine / 2), -0.01) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / 3)
		    end
		    wait(0.2)
		    Funcs.CreateSound(157878578, Character, 9000000000, 1, false)
		    Funcs.CamShake(15, 5)
		    for v19518 = 0, 50, 1 do
		        Funcs.sphereMK(2.5, -1, "Add", Torso.CFrame * CFrame.Angles(math.rad(math.random(-360, 360)), math.rad(math.random(-360, 360)), math.rad(math.random(-360, 360))), 2.5, 2.5, 25, -0.025, BrickColor.new("Really black"), 0)
		        Funcs.slash(math.random(10, 20) / 10, 5, true, "Round", "Add", "Out", Torso.CFrame * CFrame.new(0, -3, 0) * CFrame.Angles(math.rad(math.random(-30, 30)), math.rad(math.random(-30, 30)), math.rad(math.random(-40, 40))), Vector3.new(0.05, 0.01, 0.05), math.random(50, 60) / 250, BrickColor.new("Really black"))
		    end
		    Funcs.Serv("RunService").Heartbeat:Connect(function()
		        RightHip.C0 = Funcs.Clerp(Right_Hip.C0, CF(1, -1 - 0.35 * COS(Timing.Sine / 2), -0.01) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / 3)
		        LeftHip.C0 = Funcs.Clerp(LeftHip.C0, CF(-1, -1 - 0.35 * COS(Timing.Sine / 2), -0.01) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / 3)
		        RootJoint.C0 = Funcs.Clerp(RootJoint.C0, Welds.Defaults.RootJoint.C0 * CF(0, 0, 0 + 0.35 * COS(Timing.Sine / 2)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / 3)
		    end):Disconnect()
		    Movement.Attack = false
		end

		function Funcs.SwitchMode(SModeCodeName, MusicId)
			LastestMusicId = MusicId

			coroutine.resume(coroutine.create(function()
				SoundAsset = Funcs.Serv("MarketplaceService"):GetProductInfo(tonumber(MusicId))
			end))

			if SModeCodeName == "Vaporwave" then
				TMode.Value = "Vaporwave"
				TMode.ModeName = "HSC XSE-FE - Vaporwave"

				colorz.Enabled = false
				Appearance.Lighting = false

				BGMusic:Stop()
				BGMusic.Volume = 10
				BGMusic.TimePosition = 0
				BGMusic.PlaybackSpeed = 1
				BGMusic.SoundId = "rbxassetid://" .. tostring(MusicId)
				BGMusic:Play()
			elseif SModeCodeName == "AAAA" then
				TMode.Value = "AAAA"
				TMode.ModeName = "HSC XSE-FE - ???"

				colorz.Enabled = true
				Appearance.Lighting = true

				BGMusic:Stop()
				BGMusic.Volume = 10
				BGMusic.TimePosition = 0
				BGMusic.PlaybackSpeed = 1
				BGMusic.SoundId = "rbxassetid://" .. tostring(MusicId)
				BGMusic:Play()
			elseif SModeCodeName == "Rage" then
				TMode.Value = "Rage"
				TMode.ModeName = "HSC XSE-FE - Rage"

				colorz.Enabled = true
				Appearance.Lighting = true

				BGMusic:Stop()
				BGMusic.Volume = 10
				BGMusic.TimePosition = 0
				BGMusic.PlaybackSpeed = 1
				BGMusic.SoundId = "rbxassetid://" .. tostring(MusicId)
				BGMusic:Play()
