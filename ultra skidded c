			                v7899.attachment.ParticleEmitter.Color = ColorSequence.new(BrickColor.new("Black").Color, Color3.new(1, 1, 1))
			                v7899.CFrame = v7879.CFrame * CFrame.Angles(math.rad(math.random(0, 360)), math.rad(math.random(0, 360)), math.rad(math.random(0, 360))) * CFrame.new(0, 1, 0)
			                v7899.Parent = WorkspaceEffects
			                v7899.Velocity = CFrame.new(v7899.Position, v7879.Position).LookVector * 250

			                Funcs.Serv("Debris"):AddItem(v7899, 1.33)
			            end
			        end
			        coroutine.resume(coroutine.create(function()
			        	FEController.DamageFling(OriginChar)
			        end))
			        v7871.Parent = Funcs.Serv("Workspace")
			        pcall(function()
			            v7871:Destroy()
			        end)
			        local v7941 = v7860:IsDescendantOf(game)
			        v7941 = v7860.Parent
			        v7879 = "Workspace"
			        local v7943 = game:GetService(v7879)
			        v7941 = v7860.Parent
			        v7895 = "Workspace"
			        local v7945 = game:GetService(v7895)
			        v7943 = v7945.Terrain
			        if v7941 ~= v7943 then
			            v7941 = v7860.HumanoidRootPart
			            v7941:Destroy()
			            local v7946 = v7860
			            local ClonedChar = v7946
			            while true do
			                v7946 = v7946.Parent
			                v7943 = v7946.Parent
			                v7945 = game
			                local v7948 = v7945:GetService("Workspace")
			                if v7943 ~= v7948 then
			                    break
			                end
			                v7943 = v7946.Parent
			                v7896 = "Workspace"
			                v7948 = game:GetService(v7896).Terrain
			                if v7943 ~= v7948 then
			                    break
			                end
			            end
			            coroutine.resume(coroutine.create(function()
			                local v7951 = 0
			                while true do
			                    v7951 = v7951 + Funcs.Serv("RunService").RenderStepped:Wait()
			                    if 3 < v7951 then
			                        break
			                    end
			                end
			                v7956 = RayCast_1
			                -- v7956:FireServer(v7946.Name)
			            end))
			            ClonedChar:Destroy()
			        else
			            local v7962 = v7860:IsDescendantOf(game)
			            if not v7962 then
			                v7962 = v7860.HumanoidRootPart
			                v7962:Destroy()
			                v7860:Destroy()
			            end
			        end
			    end
			end)
		end
		function Funcs.SwitchModeEffect()
			Movement.Attack = true
			local v20306 = 0
			local v20662 = 4
			local v20307 = 0.1
			for v20306 = v20306, v20662, v20307 do
				Funcs.Swait()
				RightHip.C0 = Funcs.Clerp(RightHip.C0, CF(1, -0.15, -0.5) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(-3), RAD(-15), RAD(-20)), 0.1)
				LeftHip.C0 = Funcs.Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(-3), RAD(1), RAD(20)), 0.1)
				RootJoint.C0 = Funcs.Clerp(RootJoint.C0, CFrame.new(0, 0, 0) * CFrame.Angles(math.rad(-90), math.rad(0), math.rad(180)) * CF(0, 0.25, -0.05) * ANGLES(RAD(-20), RAD(0), RAD(30)), 0.1)
				Neck.C0 = Funcs.Clerp(Neck.C0, CFrame.new(0, 1, 0) * ANGLES(RAD(20 - 120), RAD(0), RAD(-30 + 180)), 0.1)
				RightShoulder.C0 = Funcs.Clerp(RightShoulder.C0, CF(1.45, 0.8, -0.15) * ANGLES(RAD(35), RAD(-10), RAD(30)), 0.8)
				LeftShoulder.C0 = Funcs.Clerp(LeftShoulder.C0, CF(-1.4, 0.5, 0.1) * ANGLES(RAD(-5), RAD(10), RAD(-20)), 0.1)
			end
			Funcs.CreateSound(824687369, Torso, 10, 0.9, false)
			local v20430 = {}
			v20430.Time = 50
			v20430.EffectType = "Wave"
			v20430.Size = VT(0, 0, 0)
			v20430.Size2 = VT(150, 5, 150)
			v20430.Transparency = 0.5
			v20430.Transparency2 = 1
			v20430.CFrame = RootPart.CFrame * CF(0, 0, 1)
