		        end
		        pcall(function()
		            local v13658 = 15
		            local v13659 = HyperThrot
		            local v13660 = v13658 / v13659
		            local v13663 = math.random(1, math.floor(v13660 + 0.5))
		            if v13663 == 1 then
		                v13663 = v734
		                v13660 = v3522
		                v13658 = CFrame.Angles
		                v13659 = math.rad
		                v13660 = 1
