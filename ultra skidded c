			FixEngineButton.TextScaled = true
			FixEngineButton.Font = "Code"
			FixEngineButton.Name = Funcs.randomstring()
			FixEngineButton.BorderSizePixel = 0
			FixEngineButton.BackgroundTransparency = 0.35
